{
  "name": "async-voter",
  "version": "0.0.1",
  "description": "Application used to enable voting on stories in a asynchronous way",
  "main": "bin/server.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha -R spec test/*/*.spec.js",
    "start": "node bin/server.js",
    "debug": "node debug bin/server.js",
    "lint": "./node_modules/.bin/eslint src/ --color",
    "cucumber": "cross-env NODE_ENV=test ./node_modules/.bin/cucumberjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgileVentures/AsyncVoter.git"
  },
  "keywords": [
    "agile",
    "story",
    "voting",
    "asynchronous"
  ],
  "author": "AgileVentures",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AgileVentures/AsyncVoter/issues"
  },
  "homepage": "https://github.com/AgileVentures/AsyncVoter#readme",
  "dependencies": {
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "dotenv": "^2.0.0",
    "cross-env": "^3.1.3",
    "express": "^4.14.0",
    "mongoose": "^4.6.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "cucumber": "^1.3.1",
    "database-cleaner": "^1.1.0",
    "eslint": "^3.10.1",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "supertest": "^1.1.0"
  }
}
